<!DOCTYPE html>
<html>
<head>
  <%= yield :header %>
  <title>StrongApe</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults, :cache => true %>
  <%= javascript_include_tag :jquery %>
  <%= stylesheet_link_tag :all, 'jquery-ui-1.8.4.custom' %>
  <%= javascript_include_tag 'jquery-1.4.2.min', 'jquery-ui-1.8.4.custom.min', 'rails' %>
  <%= csrf_meta_tag %>

<% yield :selected_navigation %>
  <div id="menu" z-index: 2;>
    <ul z-index: 2;>     
      <li z-index: 2;>
        <%= link_to "Home", "/home/index", :class => selected_navigation(:home) %></li>
      <li z-index: 2;>
        <%= link_to "Programs", athletic_programs_path, :class => selected_navigation(:athletic_programs) %></li>
      <li z-index: 2;>
        <%= link_to "Journal", date_of_workouts_path, :class => selected_navigation(:date_of_workouts) %></li>
    </ul>
  </div>
</head>
<div id="header">
  <br /><br />
</div>

<body>
  <div id="main">
    <div id="binder">
      <div id="container">
        <br /><br />
        <%= yield %>
      </div>
    </div>
  </div>
</body>

<div id="footer">
  <br /><br />
  </div>
</html>