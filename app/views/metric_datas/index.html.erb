<h1>Listing metric_datas</h1>

<table>
  <tr>
    <th>Metric</th>
    <th>Value</th>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @metric_datas.each do |metric_data| %>
  <tr>
    <td><%= metric_data.metric_id %></td>
    <td><%= metric_data.value %></td>
    <td><%= metric_data.date %></td>
    <td><%= link_to 'Show', metric_data %></td>
    <td><%= link_to 'Edit', edit_metric_data_path(metric_data) %></td>
    <td><%= link_to 'Destroy', metric_data, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Metric data', new_metric_data_path %>
